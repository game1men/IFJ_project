##testts

    defvar LF@%%%var1
    defvar LF@%%%var2

pushs int@1
pushs int@2

call %%%convertOperation
pops LF@%%%var1
write LF@%%%var1
write string@,
pops LF@%%%var2
write LF@%%%var2

popframe